{
	"name": "flarum-lang/greek",
	"type": "flarum-extension",
	"description": "Greek language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/greek",
	"keywords": [
		"locale",
		"flarum",
		"greek-locale"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Alex Ghizila",
			"email": "info@alexghizila.com",
			"homepage": "https://www.alexghizila.com/",
			"role": "Translator"
		}
	],
	"support": {
		"issues": "https://github.com/flarum-lang/greek/issues",
		"source": "https://github.com/flarum-lang/greek"
	},
	"require": {
		"flarum/core": "^1.5"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Greek",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundColor": "#ffffff",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "el",
			"title": "Greek"
		}
	}
}
